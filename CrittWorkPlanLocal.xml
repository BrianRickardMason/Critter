                        <broker>
                            <name>CrittBroker1</name>
                        </broker>
                    </brokers>
                </connections>
            </crittBroker>
        </crittBrokers>
        <critters>
            <critter>
                <crittnick>HelloCritty0</crittnick>
                <host>127.0.0.1</host>
                <crittBroker>CrittBroker0</crittBroker>
                <services>
                    <name>Poke</name>
                </services>
            </critter>
            <critter>
                <crittnick>HelloCritty1</crittnick>
