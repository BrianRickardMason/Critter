<?xml version='1.0' encoding='UTF-8'?>
<crittwork>
    <settings>
        <username>crittuser</username>
        <workingDirectory>/home/crittuser/sandbox/Critter</workingDirectory>
        <heartbeat>1</heartbeat>
        <maxDelay>2</maxDelay>
    </settings>
    <infrastructure>
        <crittBrokers>
            <crittBroker>
                <name>CrittBroker0</name>
                <host>127.0.0.1</host>
                <connections>
                    <publish>
                        <protocol>tcp://</protocol>
                        <port>11110</port>
                    </publish>
                    <subscribe>
                        <protocol>tcp://</protocol>
                        <port>22220</port>
                    </subscribe>
                    <ui>
                        <protocol>tcp://</protocol>
                        <port>33330</port>
                    </ui>
                    <brokerPublish>
                        <protocol>tcp://</protocol>
                        <port>44440</port>
                    </brokerPublish>
                    <brokers>
                        <broker>
                            <name>CrittBroker1</name>
                        </broker>
                        <broker>
                            <name>CrittBroker2</name>
                        </broker>
                    </brokers>
                </connections>
            </crittBroker>
            <crittBroker>
                <name>CrittBroker1</name>
                <host>127.0.0.1</host>
                <connections>
                    <publish>
                        <protocol>tcp://</protocol>
                        <port>11111</port>
                    </publish>
                    <subscribe>
                        <protocol>tcp://</protocol>
                        <port>22221</port>
                    </subscribe>
                    <ui>
                        <protocol>tcp://</protocol>
                        <port>33331</port>
                    </ui>
                    <brokerPublish>
                        <protocol>tcp://</protocol>
                        <port>44441</port>
                    </brokerPublish>
                    <brokers>
                        <broker>
                            <name>CrittBroker0</name>
                        </broker>
                        <broker>
                            <name>CrittBroker2</name>
                        </broker>
                    </brokers>
                </connections>
            </crittBroker>
            <crittBroker>
                <name>CrittBroker2</name>
                <host>127.0.0.1</host>
                <connections>
                    <publish>
                        <protocol>tcp://</protocol>
                        <port>11112</port>
                    </publish>
                    <subscribe>
                        <protocol>tcp://</protocol>
                        <port>22222</port>
                    </subscribe>
                    <ui>
                        <protocol>tcp://</protocol>
                        <port>33332</port>
                    </ui>
                    <brokerPublish>
                        <protocol>tcp://</protocol>
                        <port>44442</port>
                    </brokerPublish>
